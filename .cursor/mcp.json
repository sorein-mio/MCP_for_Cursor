{
  "servers": {
    "docker-mcp": {
      "command": "python",
      "args": [
        "-m",
        "docker_mcp"
      ],
      "cwd": "\/mcp-repos/docker-mcp"
    },
    "text-editor": {
      "command": "python",
      "args": [
        "-m",
        "mcp_text_editor"
      ],
      "cwd": "\/mcp-repos/mcp-text-editor"
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "ts-node",
        "index.ts"
      ],
      "cwd": "\/mcp-repos/servers/src/filesystem"
    },
    "github": {
      "command": "npx",
      "args": [
        "ts-node",
        "index.ts"
      ],
      "cwd": "\/mcp-repos/servers/src/github"
    }
  }
}
